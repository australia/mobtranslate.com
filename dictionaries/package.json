{
  "name": "dictionaries",
  "version": "1.0.0",
  "description": "Aboriginal language dictionaries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --ext .js,.jsx,.ts,.tsx . --ignore-pattern '**/*dictionary.js' --ignore-pattern 'wajarri/examples/**' --ignore-pattern 'wajarri/convert*.js' --ignore-pattern 'wajarri/import-to-sqlite.js' --ignore-pattern 'wajarri/query-database.js' --cache --cache-location .cache/eslint/.eslintcache --max-warnings 0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "languages",
    "translation",
    "aboriginal"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.8.3",
    "typescript": "^5.7.3"
  }
}
